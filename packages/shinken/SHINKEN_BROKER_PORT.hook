#!/bin/bash

case "$1" in
    alias)
        echo "TCP port the Shinken broker listens on"
    ;;
    description)
        cat << EOF
If you configured Shinken as the monitoring core, this is the
listening port of the broker .
EOF
    ;;
    default)
        echo "7772"
    ;;
    choices)
        echo "[0-9]{1,5}"
    ;;
    set)
    ;;
    depends)
    [ "$CONFIG_CORE" = shinken ]
    ;;
esac
