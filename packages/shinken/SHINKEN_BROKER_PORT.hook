#!/bin/bash

# Alias: TCP port the Shinken broker listens on
# Menu: Basic
# Description:
#  If you configured Shinken as the monitoring core, this is the
#  listening port of the broker .

case "$1" in
    default)
        echo "7772"
    ;;
    choices)
        echo "[0-9]{1,5}"
    ;;
    set)
    ;;
    depends)
    [ "$CONFIG_CORE" = shinken ]
    ;;
esac
