#!/bin/bash

# Alias: TCP port the Shinken scheduler listens on
# Menu: Basic
# Description: 
#  If you configured Shinken as the monitoring core, this is the
#  listening port of the scheduler .

case "$1" in
    default)
        echo "7768"
    ;;
    choices)
        echo "[0-9]{1,5}"
    ;;
    set)
    ;;
    depends)
    [ "$CONFIG_CORE" = shinken ]
    ;;
esac
