#!/bin/bash

case "$1" in
    alias)
        echo "Start MySQL Daemon"
    ;;
    description)
        cat << EOF
If you want to use addons based on MySQL, please activate
this option and a site specific MySQL daemon will be started.
EOF
    ;;
    default)
        echo "off"
    ;;
    choices)
        echo "on: enable"
        echo "off: disable"
    ;;
esac
