#!/bin/bash

case "$1" in
    alias)
        echo "Start Apache Daemon"
    ;;
    description)
        cat << EOF
If you don't want to use a webserver in your instance, please deactivate
this option. Otherwise a instance specific apache daemon will be started
for each instance.
EOF
    ;;
    default)
        echo "on"
    ;;
    choices)
        echo "on: enable"
        echo "off: disable"
    ;;
    set)
    ;;
esac
